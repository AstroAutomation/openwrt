/dts-v1/;

/include/ "rt5350.dtsi"

/ {
	compatible = "ralink,rt5350-soc";
	model = "Twist Speaker";
	
	twist_power_supply: power@0 {
	};
	
	tps562200_1v8: fixedregulator@0 {
	    compatible = "regulator-fixed";
	    regulator-name = "fixed-supply";
	    regulator-min-microvolt = <1800000>;
	    regulator-max-microvolt = <1800000>;
	    startup-delay-us = <70000>;
	    regulator-boot-on;
	    gpio-open-drain;
	    vin-supply = <&twist_power_supply>;
	};
	tps562200_3v3: fixedregulator@0 {
	    compatible = "regulator-fixed";
	    regulator-name = "fixed-supply";
	    regulator-min-microvolt = <3300000>;
	    regulator-max-microvolt = <3300000>;
	    startup-delay-us = <70000>;
	    regulator-boot-on;
	    gpio-open-drain;
	    vin-supply = <&twist_power_supply>;
	};
	
	palmbus@10000000 {
		spi@b00 {
			status = "okay";
			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "w25q128";
				reg = <0 0>;
				linux,modalias = "w25q128";
				spi-max-frequency = <40000000>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
				};

				partition@50000 {
					label = "nvram";
					reg = <0x50000 0x50000>;
				};
				
				partition@A0000 {
					label = "firmware";
					reg = <0xA0000 0x7b0000>;
					read-only;
				};
				
				partition@850000 {
					label = "firmware-alt";
					reg = <0x850000 0x7b0000>;
				};
				
			};
		};
		gpio1: gpio@660 {
			status = "okay";
		};
		
		i2s: i2s@a00 {
			status = "okay";
		};
		
		dma@2800 {
			status = "okay";
		};
		
		i2c@900 {
			status = "okay";
			clock-frequency = <400000>;

			mfi_auth: i2c@11 {
				compatible = "apple_auth_coproc";
				reg = <0x11>;
				    
				gpio-reset = <&gpio0>;
			};
			
			codec: tlv320aic3100@18 {
				#sound-dai-cells = <0>;
				compatible = "ti,tlv320aic3100";
				reg = <0x18>;
				
				HPVDD-supply = <&tps562200_3v3>;
				SPRVDD-supply = <&twist_power_supply>;
				SPLVDD-supply = <&twist_power_supply>;
				AVDD-supply = <&tps562200_3v3>;
				IOVDD-supply = <&tps562200_3v3>;
				DVDD-supply = <&tps562200_1v8>;
				
				//gpio-reset = <&spi_cs1>
			};

		};
	};
	
	sound {
		compatible = "simple-audio-card";
		simple-audio-card,name = "Audio-I2S";
		simple-audio-card,format = "i2s";
		simple-audio-card,bitclock-master = <&dailink0_master>;
		simple-audio-card,frame-master = <&dailink0_master>;
		simple-audio-card,widgets =
			"Speaker", "Headphone", "Headphones";
		simple-audio-card,routing =
			"Speaker", "SPK",
			"Headphones", "HP_L",
			"Headphones", "HP_R";
		simple-audio-card,mclk-fs = <256>;

		simple-audio-card,cpu {
			sound-dai = <&i2s>;
		};

		dailink0_master: simple-audio-card,codec {
			sound-dai = <&codec>;
		};
	};
	
	pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "led", "spi_cs1";
				ralink,function = "gpio";
			};
			
			uartf {
				ralink,group = "uartf";
				ralink,function = "i2s uartf";
			};
		};
	};

	ethernet@10100000 {
		mtd-mac-address = <&factory 0x4>;
		status = "disabled";
	};

	esw@10110000 {
		mediatek,portmap = <0x2f>;
		mediatek,led_polarity = <0x17>;
		status = "disabled";
	};

	wmac@10180000 {
		ralink,mtd-eeprom = <&factory 0>;
		ralink,led-polarity = <1>;
	};

	ehci@101c0000 {
		status = "disabled";
	};

	ohci@101c1000 {
		status = "disabled";
    };
};